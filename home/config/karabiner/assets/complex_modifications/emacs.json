{
  "title": "Emacs",
  "rules": [
    {
      "description": "Send return_or_enter on Ctrl+M",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send delete_or_backspace on Ctrl+H",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send delete_forward on Ctrl+D",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send right_arrow on Ctrl+F",
      "manipulators": [
        {
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send left_arrow on Ctrl+B",
      "manipulators": [
        {
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send home on Ctrl+A",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send end on Ctrl+E",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Send return_or_enter on Ctrl+M unless Terminal",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "com.apple.Terminal",
                "com.googlecode.iterm2",
                "io.alacritty"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
