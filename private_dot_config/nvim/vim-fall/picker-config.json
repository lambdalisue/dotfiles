{
  "$schema": "https://vim-fall.github.io/jsonschema/v1/picker-config.schema.json",
  "source": {
    "": {
      "defaultAction": "open",
      "actions": ["*"],
      "previewer": "text",
      "processors": ["substring_filter", "lexical_sorter"],
      "renderers": ["nerdfont"],
      "options": {
        "layout": {
          "border": "rounded"
        }
      }
    },
    "line": {
      "processors": ["substring_filter"],
      "renderers": []
    },
    "mru": {
      "processors": ["substring_filter"]
    },
    "mrw": {
      "processors": ["substring_filter"]
    },
    "mrr": {
      "processors": ["substring_filter"]
    }
  },
  "action": {
    "processors": ["substring_filter", "lexical_sorter"],
    "renderers": [],
    "options": {
      "layout": {
        "border": "rounded"
      }
    }
  }
}
