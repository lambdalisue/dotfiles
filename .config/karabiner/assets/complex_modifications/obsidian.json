{
  "title": "Obsidian",
  "rules": [
    {
      "description": "Send Escape twice on Escape",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^md\\.obsidian"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "escape"
          },
          "to": [
            {
              "key_code": "escape"
            },
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
